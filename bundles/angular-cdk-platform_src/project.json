{
  "projectType": "application",
  "root": "bundles/angular-cdk-platform_src",
  "sourceRoot": "bundles/angular-cdk-platform_src",
  "targets": {
    "build-dev": {
      "executor": "@angular-builders/custom-webpack:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "index": "",
        "main": "",
        "assets": [],
        "outputPath": "dist/bundles/angular-cdk-platform/dev",
        "tsConfig": "bundles/tsconfig.json",
        "customWebpackConfig": {
          "path": "bundles/angular-cdk-platform_src/webpack.config.js",
          "libraryName": "@angular/cdk/platform",
          "libraryTarget": "umd"
        },
        "outputHashing": "none",
        "buildOptimizer": false,
        "optimization": false,
        "vendorChunk": true,
        "extractLicenses": false,
        "sourceMap": true,
        "namedChunks": true
      }
    },
    "build-prod": {
      "executor": "@angular-builders/custom-webpack:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "index": "",
        "main": "",
        "assets": [],
        "outputPath": "dist/bundles/angular-cdk-platform/prod",
        "tsConfig": "bundles/tsconfig.json",
        "customWebpackConfig": {
          "path": "bundles/angular-cdk-platform_src/webpack.config.js",
          "libraryName": "@angular/cdk/platform",
          "libraryTarget": "umd"
        },
        "outputHashing": "none",
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "500kb",
            "maximumError": "1mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "2kb",
            "maximumError": "4kb"
          }
        ]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx build-dev angular-cdk-platform", "nx build-prod angular-cdk-platform"],
        "parallel": true
      }
    },
    "release": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["node ./tools/lib/bundle/release.js --name=@angular/cdk/platform --package-name=@angular/cdk --publish"],
        "parallel": false
      }
    }
  }
}
